        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    // Add your code here
    gs.info('runing VLM integration');
    var sinclude = new x_146833_redhatpre.CustPredictAjaxUtils();
    var cause = current.cause.toString();
    var inputType = 'image';
    if (cause.indexOf('agent') > -1) {
        inputType = 'video';
    }
    var returnNotes = 'no valid response from VLM';
    var response = sinclude.getModel(cause, inputType,'br');
    var responseObj = JSON.parse(response);
    //var product = '';
    //product= responseObj.model;
    try {
        var notes = responseObj.notes;
        var notesArray = [];
        notesArray = notes.split('videoResponse=');
        var techNotes = notesArray[0];
        var agentNotes = notesArray[1];
        if (inputType == 'image') {
			techNotes = techNotes.replace('imageResponse=','');
            returnNotes = techNotes;
        }
        if (inputType == 'video') {
            returnNotes = agentNotes;
        }
    } catch (e) {
        gs.info('error in VLM response' + e);
    }
	gs.info('returnNotes=' + returnNotes);
    current.close_notes = returnNotes;
	gs.addInfoMessage('response from VLM: ' + returnNotes);
	current.update();
})(current, previous);]]></script>
