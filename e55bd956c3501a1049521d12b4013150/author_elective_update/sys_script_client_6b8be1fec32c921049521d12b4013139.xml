        <name>PredictModelBasedOnWorkNotes</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    //Type appropriate comment here, and begin script below
    var caseNotes = g_form.getValue('cause');
    var ga = new GlideAjax('CustPredictAjaxUtils'); // getServiceInfo is the script include name 
    ga.addParam('sysparm_name', 'getModel');
    ga.addParam('sysparm_sdesc', caseNotes); // set service
    if (caseNotes.indexOf('agent') > -1) {
        ga.addParam('sysparm_type', 'video');
    }
    if (caseNotes.indexOf('tech') > -1) {
        ga.addParam('sysparm_type', 'image');
    }

    /* Call GetUserInfo.managerName() with user set to Fred Luddy and use the callback function ManagerParse() to return the result when ready */
    ga.getXMLAnswer(ManagerParse);

}

function ManagerParse(response) {
    var responseObj = JSON.parse(response);
    //var product = '';
    //product= responseObj.model;
    var notes = responseObj.notes;
    alert("responseObj" + responseObj + " notes =" + notes);
    var imageNotes;
    var videoNotes;
    var notesArray = notes.split('videoResponse=');
    imageNotes = notesArray[0];
    videoNotes = notesArray[1];
    if (caseNotes.indexOf('agent') > -1) {
        g_form.setValue('work_notes', videoNotes);
    }
    if (caseNotes.indexOf('tech') > -1) {
        g_form.setValue('work_notes', imageNotes);
    }
}]]></script>
